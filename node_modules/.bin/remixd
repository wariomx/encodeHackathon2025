#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/wario/Documents/hackathons/encode2025/encodeHackathon2025/node_modules/.pnpm/@remix-project+remixd@0.6.41_typescript@5.6.3_zod@3.24.1/node_modules/@remix-project/remixd/src/bin/node_modules:/Users/wario/Documents/hackathons/encode2025/encodeHackathon2025/node_modules/.pnpm/@remix-project+remixd@0.6.41_typescript@5.6.3_zod@3.24.1/node_modules/@remix-project/remixd/src/node_modules:/Users/wario/Documents/hackathons/encode2025/encodeHackathon2025/node_modules/.pnpm/@remix-project+remixd@0.6.41_typescript@5.6.3_zod@3.24.1/node_modules/@remix-project/remixd/node_modules:/Users/wario/Documents/hackathons/encode2025/encodeHackathon2025/node_modules/.pnpm/@remix-project+remixd@0.6.41_typescript@5.6.3_zod@3.24.1/node_modules/@remix-project/node_modules:/Users/wario/Documents/hackathons/encode2025/encodeHackathon2025/node_modules/.pnpm/@remix-project+remixd@0.6.41_typescript@5.6.3_zod@3.24.1/node_modules:/Users/wario/Documents/hackathons/encode2025/encodeHackathon2025/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/wario/Documents/hackathons/encode2025/encodeHackathon2025/node_modules/.pnpm/@remix-project+remixd@0.6.41_typescript@5.6.3_zod@3.24.1/node_modules/@remix-project/remixd/src/bin/node_modules:/Users/wario/Documents/hackathons/encode2025/encodeHackathon2025/node_modules/.pnpm/@remix-project+remixd@0.6.41_typescript@5.6.3_zod@3.24.1/node_modules/@remix-project/remixd/src/node_modules:/Users/wario/Documents/hackathons/encode2025/encodeHackathon2025/node_modules/.pnpm/@remix-project+remixd@0.6.41_typescript@5.6.3_zod@3.24.1/node_modules/@remix-project/remixd/node_modules:/Users/wario/Documents/hackathons/encode2025/encodeHackathon2025/node_modules/.pnpm/@remix-project+remixd@0.6.41_typescript@5.6.3_zod@3.24.1/node_modules/@remix-project/node_modules:/Users/wario/Documents/hackathons/encode2025/encodeHackathon2025/node_modules/.pnpm/@remix-project+remixd@0.6.41_typescript@5.6.3_zod@3.24.1/node_modules:/Users/wario/Documents/hackathons/encode2025/encodeHackathon2025/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@remix-project/remixd/src/bin/remixd.js" "$@"
else
  exec node  "$basedir/../@remix-project/remixd/src/bin/remixd.js" "$@"
fi
